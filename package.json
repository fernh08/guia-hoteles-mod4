{
  "name": "guia-hoteles",
  "version": "1.0.0",
  "description": "nuestro primer proyecto de nodejs",
  "main": "index.html",
  "scripts": {
    "dev": "lite-server",
    "start": "concurrently \"npm run watch:scss\" \"npm run dev \"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -o css/ css/ ",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "clean": "rimraf dist",
    "imagemin": "imagemin images/* --out-dir dist/images",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html && usemin about.html -d dist --htmlmin -o dist/about.html && usemin tarifas.html -d dist --htmlmin -o dist/tarifas.html && usemin contacto.html -d dist --htmlmin -o dist/contacto.html",
    "build": "npm run clean && npm run scss && npm run imagemin && npm run usemin "
  },
  "repository": {
    "type": "git",
    "url": "git+https://Fernando1491@bitbucket.org/Fernando1491/guia-hoteles.git"
  },
  "author": "Fernando E Hartl",
  "license": "ISC",
  "homepage": "https://bitbucket.org/Fernando1491/guia-hoteles#readme",
  "devDependencies": {
    "autoprefixer": "^10.0.2",
    "browser-sync": "^2.26.13",
    "copyfiles": "^2.4.0",
    "cssmin": "^0.4.3",
    "grunt": "^1.3.0",
    "grunt-contrib-cssmin": "^3.0.0",
    "grunt-contrib-sass": "^2.0.0",
    "grunt-contrib-uglify": "^5.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-postcss": "^0.9.0",
    "gulp": "^4.0.2",
    "htmlmin": "^0.0.7",
    "lite-server": "^2.0.1",
    "node-sass": "^8.0.0",
    "onchange": "^7.1.0",
    "rimraf": "^3.0.2",
    "uglify-js": "^2.4.24",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.5.4",
    "@primer/css": "^15.2.0",
    "bootstrap": "^4.5.3",
    "bootstrap-icons": "^1.1.0",
    "concurrently": "^5.3.0",
    "imagemin": "^7.0.1",
    "jquery": "^3.5.1",
    "open-iconic": "^1.1.1"
  }
}
